name = TGD Staging
description = Some SSO and other settings for staging site. Do not enable on Production site.
core = 7.x
package = TheGoodData
version = 7.x-1.0
project = tgd_staging
dependencies[] = ctools
dependencies[] = reroute_email
dependencies[] = strongarm
dependencies[] = tgd_sso
features[ctools][] = strongarm:strongarm:1
features[features_api][] = api:2
features[variable][] = reroute_email_address
features[variable][] = reroute_email_enable
features[variable][] = reroute_email_enable_message
features[variable][] = tgd_sso_auth_token
features[variable][] = tgd_sso_base_url
features[variable][] = tgd_sso_debug_enable
features[variable][] = tgd_sso_local_create
features[variable][] = tgd_sso_local_disable
features[variable][] = tgd_sso_remote_enable
features[variable][] = tgd_sso_shared_cookie
